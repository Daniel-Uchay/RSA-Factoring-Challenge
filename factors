#!/bin/python3

import sys

def factorize(n):
  for i in range(2, n):
    if n % i == 0:
      return (i, n // i)
  return (n, 1)

# get filename from command line arguments
filename = sys.argv[1]

# open file and read each line
with open(filename, 'r') as f:
  for line in f:
    n = int(line.strip())
    p, q = factorize(n)
    print(f"{n}={p}*{q}")


#!/bin/python3
import sys
import threading

# function to factorize a given number
def factorize(n):
  for i in range(2, n):
    if n % i == 0:
      return (i, n // i)
  return (n, 1)

# function to process a line from the input file
def process_line(line):
  n = int(line.strip())
  p, q = factorize(n)
  print(f"{n}={p}*{q}")

# get filename from command line arguments
filename = sys.argv[1]

# open file and read each line
with open(filename, 'r') as f:
  # create a list of threads
  threads = []
  for line in f:
    # create a thread for each line and start it
    t = threading.Thread(target=process_line, args=(line,))
    t.start()
    threads.append(t)
  # wait for all threads to finish
  for t in threads:
    t.join()

